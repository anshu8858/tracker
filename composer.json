{
    "name": "anshu8858/tracker",
    "description": "A Laravel Visitor Tracker Forked from Antonio Carlos Ribeiro",
    "keywords": ["tracker", "anlytics", "tracking", "visitor", "logging", "user agent", "mobile detection", "laravel", "pragmarx"],
    "license": "MIT",
    "authors": [
        {
            "name": "Anshu Kushawaha",
            "email": "kushawahaanshu8858@gmail.com",
            "role": "Creator & Designer"
        }
    ],

    "require": {
        "php": ">=7.2",
        "doctrine/dbal": "^2.11",
        "laravel/framework": "~7|~8",
        "pragmarx/support": "~0.8|~0.9",
        "ramsey/uuid": "^4",
        "jenssegers/agent": "~2.6",
        "ua-parser/uap-php" : "~3.4",
        "snowplow/referer-parser": "~0.1",
        "jaybizzle/crawler-detect": "~1.2",
        "psr/log": "~1.0"
    },

    "suggest": {
        "geoip/geoip": "~1.14",
        "geoip2/geoip2": "~2.0"
    },

    "require-dev": {
        "mockery/mockery": "~0.8"
    },

    "autoload": {
        "psr-4": {
            "PragmaRX\\Tracker\\": "src/"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
               "PragmaRX\\Tracker\\Vendor\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "Tracker": "PragmaRX\\Tracker\\Vendor\\Laravel\\Facade"
            }
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
